{% extends "page.twig" %}

{% block content %}

  <article class="page-about">
    {% include "about-banner.twig" with {
      fields: {
        image: post.get_field('about1_image'),
				heading: post.get_field('about1_heading'),
				text: post.get_field('about1_text')
      }
    }%}
    {% include "bios.twig" with {
      fields: {
				bios: post.get_field('about2_bio')
      }
    }%}
  </article>

{% endblock %}
